<!DOCTYPE html>
<html>
<head>
  <title>ChowSource</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav>
  <%if logged_in?%>
    <%=link_to "logout", logout_path, method:"delete"%>
  <%else%>
    <%=link_to "login", login_path%>
    <%=link_to "register", register_path%>
  <%end%>
</nav>
<%= yield %>

</body>
</html>
